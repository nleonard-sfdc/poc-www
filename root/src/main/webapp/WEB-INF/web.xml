<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Root Web App</display-name>

  <servlet>
    <servlet-name>LeadFilter</servlet-name>
    <servlet-class>servlet.LeadFilter</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LeadFilter</servlet-name>
    <url-pattern>/conf/*</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>SignupServlet</servlet-name>
    <servlet-class>servlet.SignupServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SignupServlet</servlet-name>
    <url-pattern>/leadcapture/SignupServlet</url-pattern>
  </servlet-mapping>

</web-app>
